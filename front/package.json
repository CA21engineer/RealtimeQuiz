{
  "name": "front",
  "version": "1.0.0",
  "description": "My webpack project",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "start": "next start",
    "start:server": "next start -p 3100",
    "start:ci": "npm-run-all -s clean:next build:production",
    "dev": "next dev",
    "build": "npm-run-all -s clean build:production",
    "build:dev": "next build",
    "build:production": "next build && next export -o dist/",
    "clean": "rimraf ./dist",
    "clean:next": "rimraf .next",
    "format": "npm-run-all -s format:*",
    "format:fix": "prettier --write './src/**/*.{ts, tsx, js, jsx, json}'",
    "lint": "eslint ./src --ext .ts,.tsx,.js.jsx,.json",
    "type": "tsc --noEmit",
    "storybook": "start-storybook -p 6006 -c .storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type && npm run build"
    }
  },
  "lint-staged": {
    "*.{tsx, ts, jsx, js, json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@storybook/cli": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-react-app": "^9.1.2",
    "classnames": "^2.2.6",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.3",
    "scaffdog": "^0.3.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "immer": "^7.0.9",
    "next": "^11.1.3",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-modal": "^3.11.2",
    "use-sound": "^1.0.2",
    "uuid": "^8.3.1"
  }
}
