{
  "name": "front",
  "version": "1.0.0",
  "description": "My webpack project",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "npm-run-all -s clean build:production",
    "build:production": "next build && next export -o dist/",
    "clean": "rimraf ./dist",
    "format": "npm-run-all -s format:*",
    "format:fix": "prettier --write './src/**/*.{ts, tsx, js, jsx, json}'",
    "lint": "eslint ./src --ext .ts,.tsx,.js.jsx,.json",
    "storybook": "start-storybook -p 6006 -c .storybook"
  },
  "lint-staged": {
    "*.{js, json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@storybook/cli": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.42",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.2",
    "scaffdog": "^0.2.3",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "immer": "^7.0.5",
    "next": "^9.4.4",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
